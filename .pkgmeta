package-as: GatherPanel

manual-changelog:
    filename: CHANGELOG.md
    markup-type: markdown

enable-nolib-creation: no

externals:
    Libs-release/LibStub:
        url: https://repos.wowace.com/wow/libstub/trunk
    Libs-release/LibUIDropDownMenu:
        url: https://repos.wowace.com/wow/libuidropdownmenu/trunk/LibUIDropDownMenu

move-folders:
    GatherPanel/Libs: GatherPanel/Libs-dev
    GatherPanel/Libs-release: GatherPanel/Libs

ignore:
    - Libs-dev
    - .editorconfig
    - .vscode

license-output: LICENSE
